{
  "name": "typeschema-model",
  "version": "0.0.0",
  "description": "This library contains models to represent a TypeSchema specification",
  "keywords": [
    "TypeSchema",
    "Model"
  ],
  "author": "Christoph Kappestein",
  "license": "MIT",
  "homepage": "https://www.typeschema.org/",
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    "import": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    },
    "require": {
      "types": "./dist/index.d.cts",
      "require": "./dist/index.cjs"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup src/index.ts --format cjs,esm --dts --clean --sourcemap"
  },
  "devDependencies": {
    "typescript": "^4.3",
    "tsup": "^8.3.5"
  }
}
